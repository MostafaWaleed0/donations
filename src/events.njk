<section
  class="[ hero ] [ region ] [ {{ headerBg }} ]"
  aria-labelledby="{{ heroes.events.idTitle }}"
>
  <div class="wrapper-sm flow">
    <h1 class="{{ heroes.events.heading}}" id="{{ heroes.events.idTitle }}">
      Say no to using plastic and save Earth
    </h1>
    <div class="[ hero__informations ] [ flow-space-6 ]">
      {% for item in heroes.events.info %}
      <div class="[ hero__info ] [ margin-inline-end-6 ]">
        <span
          class="[ {{ item.icon }} ] [ text-green margin-inline-end-1 ]"
          aria-hidden="true"
        ></span>
        <p class="fw-medium text-dark">{{ item.text }}</p>
      </div>
      {% endfor %}
    </div>
  </div>
</section>

